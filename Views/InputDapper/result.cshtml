
@model CityEntity
@{
    //共通ページの無効化
    Layout = "CreateLayoutView";
}


<table class="showTable">
    <tr>
        <th class="showTableCol">シティ番号</th>
        <th class="showTableCol">シティ名</th>
    </tr>
    @{
        for(int i = 0; i < ViewBag.result.Count; i++)
        {
            CityEntity product = ViewBag.result[i];
            <tr>
                <td class="showTableRow">@product.City_id</td>
                <td class="showTableRow">@product.City</td>
            </tr>

          
        }
    }
</table>

@*<a href="InputDapper">トップページに戻る</a>*@
<a asp-controller="InputDapper" asp-action="Index">トップページに戻る</a>