
@model ProductEntity




<table>
    <tr>
        <th>商品名</th>
    </tr>
    @{
        for(int i = 0; i < ViewBag.result.Count; i++)
        {
            ProductEntity product = ViewBag.result[i];
            <tr>
                <td>@product.ProductName</td>
            </tr>
        }
    }
</table>


<table class="showTableClass">
    <tr>
        <th>商品名</th>
        <th>商品金額</th>
    </tr>
    @{
        for(int i = 0; i < ViewBag.result.Count; i++)
        {
            ProductEntity product = ViewBag.result[i];
            <tr>
                <td>@product.ProductName</td>
                <td>@product.ProductPrice</td>
            </tr>

          
        }
    }
</table>